{
  "name": "raudt-kandidater-notodden-2019",
  "version": "2.0.1",
  "description": "Listekandidater for Rødt Notodden 2019",
  "private": true,
  "scripts": {
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "now-build": "next build",
    "deploy": "npm test && now --prod",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "author": "Geir Gåsodden <geir.gasodden@pythonia.no> (https://github.com/zrrrzzt)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zrrrzzt/raudt-kandidater-notodden-2019.git"
  },
  "bugs": {
    "url": "https://github.com/zrrrzzt/raudt-kandidater-notodden-2019/issues"
  },
  "homepage": "https://github.com/zrrrzzt/raudt-kandidater-notodden-2019#readme",
  "dependencies": {
    "dom-to-image": "2.6.0",
    "file-saver": "2.0.5",
    "next": "12.1.0",
    "next-offline": "5.0.5",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "ava": "3.15.0",
    "babel-eslint": "10.1.0",
    "coveralls": "3.1.0",
    "nyc": "15.1.0",
    "standard": "16.0.3"
  },
  "standard": {
    "ignore": [
      "/static/*.js"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    }
  },
  "engines": {
    "node": ">= 12.16.3"
  }
}
